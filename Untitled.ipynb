   "outputs": [],
   "source": [
    "cap=cv2.videocapture(0)\n",
    "while True:\n",
    "    ret,frame=cap.read()\n",
    "    gray_frame=cv2.cvtcolor(frame,cv2.COLOR_BGR2RGB)\n",
    "if ret=False:\n",
    "    continue\n",
    "cv2.imshow(\"video frame\",frame)\n",
    "cv2.imshow(\"gray frame\",gray_frame)"
   ]
  }
